{
  "name": "canadian-dental-care-plan",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "postinstall": "patch-package --patch-dir other/patches",
    "build": "npm run build:application && npm run build:server",
    "build:application": "react-router build",
    "build:server": "vite build --config ./vite.server.config.ts",
    "dev": "nodemon --config nodemon.json",
    "start": "cross-env NODE_ENV=production node --import ./build/server/opentelemetry.server.js ./build/server/express.server.js",
    "checks": "node --experimental-strip-types ./scripts/checks-runner.ts",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ./",
    "lint:summary": "eslint --format summary .",
    "typecheck": "react-router typegen && tsc",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install chromium",
    "test:e2e:smoke": "playwright test --grep @smoke",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ui": "vitest --ui --coverage.enabled=true"
  },
  "dependencies": {
    "@date-fns/utc": "^2.1.1",
    "@dts-stn/health-checks": "^2.0.0",
    "@dts-stn/invariant": "^1.0.0",
    "@faker-js/faker": "^10.3.0",
    "@fortawesome/fontawesome-svg-core": "^7.2.0",
    "@fortawesome/free-brands-svg-icons": "^7.2.0",
    "@fortawesome/free-regular-svg-icons": "^7.2.0",
    "@fortawesome/free-solid-svg-icons": "^7.2.0",
    "@fortawesome/react-fontawesome": "^3.2.0",
    "@hcaptcha/react-hcaptcha": "^2.0.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.70.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.212.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.212.0",
    "@opentelemetry/sdk-metrics": "^2.5.1",
    "@opentelemetry/sdk-node": "^0.212.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-router/node": "^7.13.0",
    "@react-router/serve": "^7.13.0",
    "bowser": "^2.14.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "fast-xml-parser": "^5.3.6",
    "file-type": "^21.3.0",
    "filesize": "^11.0.13",
    "i18next": "^25.8.10",
    "i18next-http-backend": "^3.0.2",
    "inversify": "^7.11.0",
    "isbot": "^5.1.35",
    "jose": "^6.1.3",
    "libphonenumber-js": "^1.12.37",
    "markdown-to-jsx": "^9.7.4",
    "micro-memoize": "^5.1.1",
    "mime": "^4.1.0",
    "minimatch": "^10.2.1",
    "moderndash": "^4.0.0",
    "morgan": "^1.10.1",
    "nanoid": "^5.1.6",
    "oxide.ts": "^1.1.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-i18next": "^16.5.4",
    "react-idle-timer": "^5.7.2",
    "react-number-format": "^5.4.4",
    "react-phone-number-input": "^3.4.14",
    "react-router": "^7.13.0",
    "redis": "^5.11.0",
    "reflect-metadata": "^0.2.2",
    "sanitize-filename-ts": "^1.0.2",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^3.4.1",
    "undici": "^7.22.0",
    "use-deep-compare": "^1.3.0",
    "validator": "^13.15.26",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-error-format": "^3.0.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@inquirer/checkbox": "^5.0.7",
    "@playwright/test": "^1.58.2",
    "@react-router/dev": "^7.13.0",
    "@react-router/express": "^7.13.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/compression": "^1.8.1",
    "@types/express-session": "^1.18.2",
    "@types/jmespath": "^0.15.2",
    "@types/jsdom": "^27.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/source-map-support": "^0.5.10",
    "@types/validator": "^13.15.10",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.9",
    "@vitest/ui": "^4.0.18",
    "commander": "^14.0.3",
    "eslint": "^9.39.2",
    "eslint-formatter-summary": "^2.0.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unicorn": "^63.0.0",
    "globals": "^17.3.0",
    "jmespath": "^0.16.0",
    "jsdom": "^28.1.0",
    "nodemon": "^3.1.11",
    "ora": "^9.3.0",
    "patch-package": "^8.0.1",
    "playwright-teamcity-reporter": "^1.0.5",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.21.0",
    "type-fest": "^5.4.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1",
    "vite-plugin-static-copy": "^3.2.0",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18",
    "vitest-mock-extended": "^3.1.0"
  },
  "engines": {
    "node": ">=24.13.0",
    "npm": ">=11.6.2"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 256,
    "importOrder": [
      "^react(-dom/.*)?$",
      "^@testing-library/(.*)$",
      "^[@]?react-router(/.*)?",
      "<THIRD_PARTY_MODULES>",
      "^[\\.]",
      "^[~]"
    ],
    "importOrderParserPlugins": [
      "typescript",
      "jsx",
      "decorators-legacy"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ]
  }
}
