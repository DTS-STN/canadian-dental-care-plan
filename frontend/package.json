{
  "name": "canadian-dental-care-plan",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "postinstall": "patch-package --patch-dir other/patches",
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "cross-env NODE_ENV=production node ./build/server/index.js",
    "check:all": "npm run format:check && npm run lint && npm run typecheck",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --ignore-path ./.gitignore --cache --cache-location ./node_modules/.cache/eslint ./",
    "typecheck": "tsc",
    "test:all": "npm run test:unit run && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install chromium",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ui": "vitest --ui --coverage.enabled=true"
  },
  "dependencies": {
    "@date-fns/utc": "^1.2.0",
    "@faker-js/faker": "^9.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@hcaptcha/react-hcaptcha": "^1.11.0",
    "@mswjs/data": "^0.16.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.53.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.53.0",
    "@opentelemetry/sdk-metrics": "^1.26.0",
    "@opentelemetry/sdk-node": "^0.53.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@remix-run/css-bundle": "^2.11.2",
    "@remix-run/node": "^2.11.2",
    "@remix-run/react": "^2.11.2",
    "@remix-run/serve": "^2.11.2",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "fast-json-patch": "^3.1.1",
    "fast-xml-parser": "^4.5.0",
    "i18next": "^23.14.0",
    "i18next-fs-backend": "^2.3.2",
    "i18next-http-backend": "^2.6.1",
    "ioredis": "^5.4.1",
    "isbot": "^5.1.17",
    "jose": "^5.8.0",
    "libphonenumber-js": "^1.11.7",
    "markdown-to-jsx": "^7.5.0",
    "moderndash": "^3.12.0",
    "moize": "^6.1.6",
    "morgan": "^1.10.0",
    "msw": "^2.4.2",
    "notistack": "^3.0.1",
    "oauth4webapi": "^2.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.1",
    "react-idle-timer": "^5.7.2",
    "react-number-format": "^5.4.1",
    "react-phone-number-input": "^3.4.5",
    "remix-create-express-app": "^0.4.5",
    "sanitize-filename-ts": "^1.0.2",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^2.5.2",
    "tiny-invariant": "^1.3.3",
    "use-deep-compare": "^1.3.0",
    "validator": "^13.12.0",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-error-format": "^3.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.1",
    "@remix-run/dev": "^2.11.2",
    "@remix-run/testing": "^2.11.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/compression": "^1.7.5",
    "@types/jsdom": "^21.1.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/source-map-support": "^0.5.10",
    "@types/validator": "^13.12.1",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^25.0.0",
    "patch-package": "^8.0.0",
    "playwright-teamcity-reporter": "^1.0.3",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "remix-express-dev-server": "^0.4.4",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 256,
    "importOrder": [
      "^react$",
      "^@testing-library/(.*)$",
      "^@remix(.*)$",
      "<THIRD_PARTY_MODULES>",
      "^[(.|~)/]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ]
  }
}
