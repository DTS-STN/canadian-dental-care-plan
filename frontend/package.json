{
  "name": "canadian-dental-care-plan",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "postinstall": "patch-package --patch-dir other/patches",
    "build": "npm run build:application && npm run build:server",
    "build:application": "remix vite:build",
    "build:server": "vite build --config ./vite.server.config.ts",
    "dev": "cross-env NODE_ENV=development nodemon --env-file .env --watch .env --ext ts --watch './app/.server/express-server/**/*.ts' --exec tsx ./app/.server/express-server/express.server.ts",
    "start": "cross-env NODE_ENV=production node ./build/express.server.mjs",
    "check:all": "npm run format:check && npm run lint && npm run typecheck",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ./",
    "typecheck": "tsc",
    "test:all": "npm run test:unit run && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install chromium",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ui": "vitest --ui --coverage.enabled=true"
  },
  "dependencies": {
    "@date-fns/utc": "^2.1.0",
    "@dts-stn/health-checks": "^2.0.0",
    "@faker-js/faker": "^9.3.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.1",
    "@fortawesome/free-regular-svg-icons": "^6.7.1",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@hcaptcha/react-hcaptcha": "^1.11.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.55.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.55.0",
    "@opentelemetry/sdk-metrics": "^1.28.0",
    "@opentelemetry/sdk-node": "^0.55.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@remix-run/node": "^2.15.0",
    "@remix-run/react": "^2.15.0",
    "@remix-run/route-config": "^2.15.0",
    "@remix-run/serve": "^2.15.0",
    "clsx": "^2.1.1",
    "compression": "^1.7.5",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "diff": "^7.0.0",
    "express": "^4.21.1",
    "fast-xml-parser": "^4.5.0",
    "i18next": "^24.0.5",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-backend": "^3.0.1",
    "inversify": "^6.1.6",
    "inversify-logger-middleware": "^3.1.0",
    "ioredis": "^5.4.1",
    "isbot": "^5.1.17",
    "jose": "^5.9.6",
    "libphonenumber-js": "^1.11.16",
    "markdown-to-jsx": "^7.7.1",
    "minimatch": "^10.0.1",
    "moderndash": "^4.0.0",
    "moize": "^6.1.6",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.3",
    "react-idle-timer": "^5.7.2",
    "react-number-format": "^5.4.2",
    "react-phone-number-input": "^3.4.10",
    "reflect-metadata": "^0.2.2",
    "sanitize-filename-ts": "^1.0.2",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^2.5.5",
    "tiny-invariant": "^1.3.3",
    "use-deep-compare": "^1.3.0",
    "validator": "^13.12.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-error-format": "^3.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@playwright/test": "^1.49.0",
    "@remix-run/dev": "^2.15.0",
    "@remix-run/express": "^2.15.0",
    "@remix-run/testing": "^2.15.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/compression": "^1.7.5",
    "@types/diff": "^6.0.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jsdom": "^21.1.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/source-map-support": "^0.5.10",
    "@types/validator": "^13.12.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.16.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.13.0",
    "jsdom": "^25.0.1",
    "msw": "^2.6.6",
    "nodemon": "^3.1.7",
    "patch-package": "^8.0.0",
    "playwright-teamcity-reporter": "^1.0.4",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.16",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "type-fest": "^4.30.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.8",
    "vitest-mock-extended": "^2.0.2"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 256,
    "importOrder": [
      "reflect-metadata",
      "^react$",
      "^@testing-library/(.*)$",
      "^@remix(.*)$",
      "<THIRD_PARTY_MODULES>",
      "^[(.|~)/]"
    ],
    "importOrderParserPlugins": [
      "typescript",
      "jsx",
      "decorators-legacy"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ]
  },
  "overrides": {
    "@remix-run/dev": "^2.15.0",
    "@remix-run/react": "^2.15.0"
  }
}
