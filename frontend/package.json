{
  "name": "canadian-dental-care-plan",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "postinstall": "patch-package --patch-dir other/patches",
    "build": "npm run build:application && npm run build:server",
    "build:application": "react-router build",
    "build:server": "vite build --config ./vite.server.config.ts",
    "dev": "nodemon --config nodemon.json",
    "start": "cross-env NODE_ENV=production node --import ./build/server/opentelemetry.server.js ./build/server/express.server.js",
    "checks": "node --experimental-strip-types ./scripts/checks-runner.ts",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ./",
    "lint:summary": "eslint --format summary .",
    "typecheck": "react-router typegen && tsc",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install chromium",
    "test:e2e:smoke": "playwright test --grep @smoke",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ui": "vitest --ui --coverage.enabled=true"
  },
  "dependencies": {
    "@date-fns/utc": "^2.1.1",
    "@dts-stn/health-checks": "^2.0.0",
    "@dts-stn/invariant": "^1.0.0",
    "@faker-js/faker": "^9.9.0",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-regular-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@hcaptcha/react-hcaptcha": "^1.12.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.203.0",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "@rollup/wasm-node": "^4.46.2",
    "bowser": "^2.11.0",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "cross-env": "^10.0.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "fast-xml-parser": "^5.2.5",
    "i18next": "^25.3.2",
    "i18next-http-backend": "^3.0.2",
    "inversify": "^7.6.1",
    "isbot": "^5.1.29",
    "jose": "^6.0.12",
    "libphonenumber-js": "^1.12.10",
    "markdown-to-jsx": "^7.7.12",
    "minimatch": "^10.0.3",
    "moderndash": "^4.0.0",
    "moize": "^6.1.6",
    "morgan": "^1.10.1",
    "oxide.ts": "^1.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.6.1",
    "react-idle-timer": "^5.7.2",
    "react-number-format": "^5.4.4",
    "react-phone-number-input": "^3.4.12",
    "react-router": "^7.7.1",
    "redis": "^5.6.1",
    "reflect-metadata": "^0.2.2",
    "sanitize-filename-ts": "^1.0.2",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^3.3.1",
    "undici": "^7.12.0",
    "use-deep-compare": "^1.3.0",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-error-format": "^3.0.2",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@inquirer/checkbox": "^4.2.0",
    "@playwright/test": "^1.54.1",
    "@react-router/dev": "^7.7.1",
    "@react-router/express": "^7.7.1",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/compression": "^1.8.1",
    "@types/express-session": "^1.18.2",
    "@types/jmespath": "^0.15.2",
    "@types/jsdom": "^21.1.7",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.16.5",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/source-map-support": "^0.5.10",
    "@types/validator": "^13.15.2",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "commander": "^14.0.0",
    "eslint": "^9.32.0",
    "eslint-formatter-summary": "^2.0.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^16.3.0",
    "jmespath": "^0.16.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "ora": "^8.2.0",
    "patch-package": "^8.1.0-canary.1",
    "playwright-teamcity-reporter": "^1.0.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vite-plugin-static-copy": "^3.1.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 256,
    "importOrder": [
      "^react(-dom/.*)?$",
      "^@testing-library/(.*)$",
      "^[@]?react-router(/.*)?",
      "<THIRD_PARTY_MODULES>",
      "^[\\.]",
      "^[~]"
    ],
    "importOrderParserPlugins": [
      "typescript",
      "jsx",
      "decorators-legacy"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ]
  },
  "overrides": {
    "vite": {
      "rollup": "$@rollup/wasm-node"
    }
  }
}
