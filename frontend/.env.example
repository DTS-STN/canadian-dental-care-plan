LOOKUP_SVC_ALL_FEDERAL_SOCIAL_PROGRAMS_CACHE_TTL_SECONDS=3600
LOOKUP_SVC_ALL_GENDER_TYPES_CACHE_TTL_SECONDS=3600
LOOKUP_SVC_ALL_INDIGENOUS_GROUP_TYPES_CACHE_TTL_SECONDS=3600
LOOKUP_SVC_ALL_INDIGENOUS_TYPES_CACHE_TTL_SECONDS=3600
LOOKUP_SVC_ALL_LAST_TIME_DENTIST_VISIT_TYPES_CACHE_TTL_SECONDS=3600
LOOKUP_SVC_ALL_MARITAL_STATUSES_CACHE_TTL_SECONDS=3600
LOOKUP_SVC_ALL_MOUTH_PAIN_TYPES_CACHE_TTL_SECONDS=3600
LOOKUP_SVC_ALL_PREFERRED_COMMUNICATION_METHODS_CACHE_TTL_SECONDS=3600
LOOKUP_SVC_ALL_PREFERRED_LANGUAGES_CACHE_TTL_SECONDS=3600
LOOKUP_SVC_ALL_PROVINCIAL_TERRITORIAL_SOCIAL_PROGRAMS_CACHE_TTL_SECONDS=3600
LOOKUP_SVC_ALL_REGIONS_CACHE_TTL_SECONDS=3600
LOOKUP_SVC_ALL_SEX_AT_BIRTH_TYPES_CACHE_TTL_SECONDS=3600
LOOKUP_SVC_PREFERRED_LANGUAGE_CACHE_TTL_SECONDS=3600
GET_ALL_LETTER_TYPES_CACHE_TTL_SECONDS=3600

