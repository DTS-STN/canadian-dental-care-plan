# Example .env configuration file.
# Copy this file to `.env` and edit as required.

# current environment in which the application is running; valid values are: development, test, production
# (required; use the example below)
NODE_ENV=development


# application log level; valid values are: audit, error, warn, info, debug, and trace
# (optional; default: info)
LOG_LEVEL=debug


# enable/disable the persistent audit log
# (optional; default: true)
AUDIT_LOG_ENABLED=true
# directory in which to store audit logs
# (optional; default: logs)
AUDIT_LOG_DIRNAME=logs
# name of the audit log file; will be appended with '_{date}_{hostname}.log'
# (optional; default: audit)
AUDIT_LOG_FILENAME=audit


# i18next debug flag
# (optional; default: false)
I18NEXT_DEBUG=false


# public key JWT -- single-line public key without BEGIN/END PUBLIC KEY headers and footers
# ⚠️ Note: this must be in PEM format ⚠️
# (required; use the example below)
AUTH_JWT_PUBLIC_
# private key JWT -- single-line private key without BEGIN/END PRIVATE KEY headers and footers
# ⚠️ Note: this must be in PKCS#8 format ⚠️
# (required; use the example below)
AUTH_JWT_PRIVATE_
# logout redirect URL -- the URL to redirect to when logging out
# (required; use the example below)
AUTH_LOGOUT_REDIRECT_URL=http://localhost:3000/
# RAOIDC base URL -- used as a base URL for all RAOIDC calls
# (required; use the example below)
AUTH_RAOIDC_BASE_URL=http://localhost:3000/oidc
# RAOIDC client ID -- the OAuth client id used for all RAOIDC calls
# (required; use the example below)
AUTH_RAOIDC_CLIENT_ID=CDCP
# RASCL logout URL -- used when no RAOIDC session is found during logout
# (required; use the example below)
AUTH_RASCL_LOGOUT_URL=http://localhost:3000/


# Application feature flags -- see ./app/utils/env-utils.server.ts for valid values
# (optional; default: "")
ENABLED_FEATURES=address-validation,hcaptcha,view-letters,view-letters-online-application,status,view-payload,stub-login,demographic-survey

# GC Notify email notifications URL
# (optional; default: https://api.notification.canada.ca/v2/notifications/email)
GC_NOTIFY_EMAIL_NOTIFICATIONS_URL=https://api.notification.canada.ca/v2/notifications/email
# GC Notify API key
# (required; use the example below)
GC_NOTIFY_API_KEY=00000000000000000000000000000000
# GC Notify English email template ID
# (optional; default: 8b77428b-56a5-415c-9b48-ef1f10c128e7)
GC_NOTIFY_ENGLISH_TEMPLATE_ID=8b77428b-56a5-415c-9b48-ef1f10c128e7
# GC Notify French email template ID
# (optional; default: 2991903c-56f9-4c4e-af2e-db3ecf04b44c)
GC_NOTIFY_FRENCH_TEMPLATE_ID=2991903c-56f9-4c4e-af2e-db3ecf04b44c

# hCaptcha maximum allowed score denoting malicious activity
# (optional; default: 0.79)
HCAPTCHA_MAX_SCORE=0.79
# hCaptcha secret key -- used for server-side verifification
# (optional; default: 0x0000000000000000000000000000000000000000)
HCAPTCHA_SECRET_KEY=0x0000000000000000000000000000000000000000
# hCaptcha site key -- used to integrate hCaptcha to a webpage
# (optional; default: 20000000-ffff-ffff-ffff-000000000002)
HCAPTCHA_SITE_KEY=20000000-ffff-ffff-ffff-000000000002
# hCaptcha verify URL -- used to verify hCaptcha tokens
# (optional; default: https://api.hcaptcha.com/siteverify)
HCAPTCHA_VERIFY_URL=https://api.hcaptcha.com/siteverify


# HTTP proxy URL -- used to proxy specified outgoing HTTP calls
# (optional; default: undefined)
HTTP_PROXY_URL=

# HTTP proxy TLS timeout -- used to specifiy the timeout in milliseconds
# (optional; default: 30000)
HTTP_PROXY_TLS_TIMEOUT=30000


# Interop API base url
# (required; use the example below)
INTEROP_API_BASE_URI=https://api.example.com
# Interop API subscription key
# (required; use the example below)
INTEROP_API_SUBSCRIPTION_KEY=00000000000000000000000000000000
# Interop's Applicant API base url
# (optional; default: undefined)
INTEROP_APPLICANT_API_BASE_URI=
# Interop's Applicant API subscription key
# (optional; default: undefined)
INTEROP_APPLICANT_API_SUBSCRIPTION_KEY=
# Interop's Benefit Application API base url
# (optional; default: undefined)
INTEROP_BENEFIT_APPLICATION_API_BASE_URI=
# Interop's Benefit Application API subscription key
# (optional; default: undefined)
INTEROP_BENEFIT_APPLICATION_API_SUBSCRIPTION_KEY=
# Interop's CCT API base url
# (optional; default: undefined)
INTEROP_CCT_API_BASE_URI=
# Interop's CCT API subscription key
# (optional; default: undefined)
INTEROP_CCT_API_SUBSCRIPTION_KEY=
# Interop's CCT API community header value; linked to the specific vault database for the given community
# (optional; default: CDCP)
INTEROP_CCT_API_COMMUNITY=CDCP
# Interop's Status Check API base url
# (optional; default: undefined)
INTEROP_STATUS_CHECK_API_BASE_URI=
# Interop's Status Check API subscription key
# (optional; default: undefined)
INTEROP_STATUS_CHECK_API_SUBSCRIPTION_KEY=

# Config that holds the date used for the application year request. This date can be overridden for simulation or testing purposes -- ex. 2025-03-05
# (optional; default: undefined)
#APPLICATION_YEAR_REQUEST_DATE=2025-03-05

# OpenTelemetry log level; valid values are: none, error, warn, info, debug, verbose, all
# (optional; default: info)
OTEL_LOG_LEVEL=info
# OpenTelemetry API key
# (optional; default: undefined)
OTEL_API_KEY=
# OpenTelemetry source environment -- ex: dev, test, prod
# (optional; default: localhost)
OTEL_ENVIRONMENT=localhost
# OpenTelemetry metrics endpoint -- ex: https://example.com/e/00000000-0000-0000-0000-000000000000/api/v2/otlp/v1/metrics
# (optional; default: undefined)
OTEL_METRICS_ENDPOINT=
# OpenTelemetry metrics export interval -- how often the metrics are pushed to Dynatrace
# (optional; default: 60000)
OTEL_METRICS_EXPORT_INTERVAL_MILLIS=60000
# OpenTelemetry metrics export timeout -- how long to wait for a response from Dynatrace
# (optional; default: 30000)
OTEL_METRICS_EXPORT_TIMEOUT_MILLIS=30000
# OpenTelemetry service name
# (optional; default: canadian-dental-care-program)
OTEL_SERVICE_NAME=canadian-dental-care-plan
# OpenTelemetry traces endpoint -- ex: https://example.com/e/00000000-0000-0000-0000-000000000000/api/v2/otlp/v1/traces
# (optional; default: undefined)
OTEL_TRACES_ENDPOINT=
# Use OpenTelemetry console exporters; useful for troubleshooting/debugging
# (optional; default: false)
OTEL_USE_CONSOLE_EXPORTERS=false

# session storage type; valid values are: file, redis
# (optional; default: file)
SESSION_STORAGE_TYPE=file
# session expiry time in seconds
# (optional; default: 1200)
SESSION_EXPIRES_SECONDS=1200
# session cookie name
# (optional; default: __CDCP||session)
SESSION_COOKIE_NAME=__CDCP||session
# session cookie domain
# (optional; default: undefined)
SESSION_COOKIE_DOMAIN=localhost
# session cookie path
# (optional; default: /)
SESSION_COOKIE_PATH=/
# session cookie samesite value
# (optional; default: lax)
SESSION_COOKIE_SAME_SITE=lax
# session cookie encryption secret
# (optional; default: a random UUID)
SESSION_COOKIE_SECRET=0123456789ABCDEF
# session cookie httpOnly flag
# (optional; default: true)
SESSION_COOKIE_HTTP_ONLY=true
# session cookie secure flag
# (optional; default: true)
SESSION_COOKIE_SECURE=true
# location of session files when using file storage
# (optional; default: ./node_modules/.cache/sessions/)
SESSION_FILE_DIR=./node_modules/.cache/sessions/
# the time before a user is considered idle in seconds
# (optional; default: 1140)
SESSION_TIMEOUT_SECONDS=1140
# how long before idle to emit the onPrompt event in seconds
# (optional; default: 300)
SESSION_TIMEOUT_PROMPT_SECONDS=300

# redis sentinel name -- used for HA configurations
# (optional; default: undefined)
REDIS_SENTINEL_NAME=
# redis sentinel host -- used for HA configurations
# (optional; default: undefined)
REDIS_SENTINEL_HOST=
# redis sentinel port -- used for HA configurations
# (optional; default: undefined)
REDIS_SENTINEL_PORT=
# redis standalone host -- used for non-HA configurations
# (optional; default: localhost)
REDIS_STANDALONE_HOST=localhost
# redis standalone port -- used for non-HA configurations
# (optional; default: localhost)
REDIS_STANDALONE_PORT=6379
# redis server username
# (optional; default: undefined)
REDIS_USERNAME=username
# redis server password
# (optional; default: undefined)
REDIS_PASSWORD=password
# Command timeout for Redis operations in seconds (default: 1).
# Specifies the maximum time to wait before a command times out.
REDIS_COMMAND_TIMEOUT_SECONDS=


# enabled MSW mocks -- comma separated list of mocks
# (optional; default: undefined)
ENABLED_MOCKS=cct,power-platform,raoidc,status-check,wsaddress
# allowed OIDC redirects -- list of allowed OIDC redirect URLs when mocking RAOIDC
# (optional; default: http://localhost:3000/auth/callback/raoidc)
MOCK_AUTH_ALLOWED_REDIRECTS=http://localhost:3000/auth/callback/raoidc

# ECAS base URL -- used as a base URL for ECAS
# (optional; default https://srv136.services.gc.ca/ecas-seca/rascl/SCL)
ECAS_BASE_URI=https://srv136.services.gc.ca/ecas-seca/rascl/SCL

# SCCH/MSCA-D base URL -- used as a base URL for all SCCH calls
# (optional; default https://service.canada.ca)
SCCH_BASE_URI=https://secure-client-hub-dev.bdm.dshp-phdn.net

# English Header Logo URL -- used as a link when clicking header logo
# (optional; default https://canada.ca/en)
HEADER_LOGO_URL_EN=https://canada.ca/en

# French Header Logo URL -- used as a link when clicking header logo
# (optional; default https://canada.ca/fr)
HEADER_LOGO_URL_FR=https://canada.ca/fr

